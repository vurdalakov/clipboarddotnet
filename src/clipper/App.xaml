<Application x:Class="Vurdalakov.ClipboardDotNet.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Vurdalakov.ClipboardDotNet"
             xmlns:vurdalakov="clr-namespace:Vurdalakov"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vurdalakov:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
            <vurdalakov:BooleanToVisibilityConverter Reverse="True" x:Key="BooleanToVisibilityReverseConverter" />

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Vurdalakov/GeneralResourceDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
